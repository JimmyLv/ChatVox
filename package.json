{
  "name": "supabase-with-openai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run embeddings && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "embeddings": "tsx lib/generate-embeddings.ts",
    "test:extract": "tsx lib/langchain/extractSrt.ts",
    "db:link": "source .env && npx supabase link --project-ref $SUPABASE_PROJECT_ID",
    "db:migrate": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:types": "source .env && npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > schemas/supabase.ts"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf",
    "sqlKeywordCase": "lower"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-toast": "^1.1.3",
    "@supabase/auth-helpers-nextjs": "^0.6.0",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.3.5",
    "@supabase/auth-ui-shared": "^0.1.3",
    "@supabase/gotrue-js": "^2.22.0",
    "@supabase/supabase-js": "^2.13.0",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "class-variance-authority": "^0.5.2",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "common-tags": "^1.8.2",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "focus-trap-react": "^10.1.1",
    "framer-motion": "^10.12.2",
    "get-video-id": "^3.6.5",
    "github-slugger": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "langchain": "^0.0.56",
    "lucide-react": "^0.128.0",
    "marked-react": "^1.3.2",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-mdx": "^2.0.0",
    "mdast-util-to-markdown": "^1.5.0",
    "mdast-util-to-string": "^3.1.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "next": "13.2.4",
    "openai": "^3.2.1",
    "prop-types": "^15.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-type-animation": "^2.1.2",
    "srt-parser-2": "^1.2.2",
    "sse.js": "^0.6.1",
    "subtitle": "^4.2.1",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.0.2",
    "unist-builder": "^3.0.1",
    "unist-util-filter": "^4.0.1",
    "youtube-captions-scraper": "^2.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/estree": "^1.0.0",
    "@types/mdast": "^3.0.11",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.6"
  }
}
